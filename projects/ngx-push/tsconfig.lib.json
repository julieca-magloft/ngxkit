{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/lib",
    "declarationMap": true,
    "target": "ES2015",
    "declaration": true,
    "inlineSources": true,
    "lib": ["dom", "es2020", "WebWorker"],
    "skipLibCheck": true
  },
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true,
    "enableResourceInlining": true
  },
  "exclude": ["node_modules", "../../node_modules"]
}
